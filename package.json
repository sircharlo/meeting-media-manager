    "pretty-bytes": "^7.1.0",
    "quasar": "^2.18.5",
    "sanitize-filename": "^1.6.3",
    "upath": "^2.0.1",
    "vue": "^3.5.21",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "vue-zoomable": "^1.2.8"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "@eslint/compat": "^1.3.2",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@pinia/testing": "^1.0.2",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/icongenie": "^4.0.0",
    "@quasar/quasar-app-extension-qmarkdown": "^2.0.5",
