{
  "name": "meeting-media-manager",
  "version": "22.7.1",
  "description": "Meeting Media Manager",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircharlo/meeting-media-manager.git"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@popperjs/core": "^2.11.5",
    "adm-zip": "^0.5.9",
    "aspectratio": "^2.2.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "bootstrap-dark-5": "^1.1.3",
    "dayjs": "^1.11.3",
    "electron-updater": "^5.2.0",
    "escape-html": "^1.0.3",
    "eslint": "^8.19.0",
    "fast-glob": "^3.2.11",
    "fast-xml-parser": "^4.0.9",
    "flatpickr": "^4.6.13",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "h264-mp4-encoder": "^1.0.12",
    "i18n": "^0.15.0",
    "image-size": "^1.0.2",
    "inputmask": "^5.0.7",
    "is-audio": "^0.0.0",
    "is-image": "^3.1.0",
    "is-video": "^1.0.1",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.13.2",
    "obs-websocket-js": "^4.0.3",
    "os": "^0.1.2",
    "pdfjs-dist": "^2.14.305",
    "select2": "^4.1.0-rc.0",
    "sql.js": "^1.7.0",
    "upath": "^2.0.1"
  },
  "build": {
    "appId": "sircharlo.meeting-media-manager",
    "productName": "Meeting Media Manager",
    "nsis": {
      "guid": "d9bcf07e-55a1-51f4-83c6-e268a864b225",
      "oneClick": false
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico",
      "publish": ["github"]
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage",
      "icon": "build/icon.png",
      "publish": ["github"]
    },
    "mac": {
      "target": {
        "target": "dmg",
        "arch": ["universal"]
      },
      "publish": ["github"]
    },
    "buildDependenciesFromSource": true
  },
  "devDependencies": {
    "electron": "^19.0.8",
    "electron-builder": "^23.3.1"
  },
  "scripts": {
    "lint:js": "eslint --fix --ext \".js\" .",
    "start-debug": "electron .  --trace-warnings > ./app-run.log  2>&1",
    "start": "electron .",
    "pack-win": "electron-builder --win",
    "pack-win-test": "electron-builder --dir --win",
    "pack-linux-test": "electron-builder --dir --linux",
    "pack-linux": "electron-builder --linux",
    "pack-mac": "electron-builder --mac",
    "publish": "electron-builder --win -p always",
    "publish-linux": "electron-builder --linux -p always",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder"
  },
  "eslintIgnore": ["docs", "node_modules"],
  "author": "sircharlo",
  "license": "ISC"
}
