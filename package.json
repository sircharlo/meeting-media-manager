{
  "name": "jw-meeting-media-fetcher",
  "version": "0.8.92",
  "description": "JW Meeting Media Fetcher",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircharlo/jw-meeting-media-fetcher.git"
  },
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-dark-4": "^0.0.9",
    "dayjs": "^1.10.4",
    "electron-updater": "^4.3.8",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.6",
    "graceful-fs": "^4.2.6",
    "is-port-reachable": "^3.0.0",
    "jquery": "^3.6.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.6.347",
    "popper.js": "^1.16.1",
    "select2": "^4.1.0-rc.0",
    "sql.js": "^1.5.0",
    "ssh2-sftp-client": "^6.0.1",
    "zip-local": "^0.3.4"
  },
  "build": {
    "appId": "sircharlo.jw-meeting-media-fetcher",
    "productName": "JW Meeting Media Fetcher",
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "category": "Utilities",
      "target": "AppImage",
      "icon": "build/icon.png",
      "publish": [
        "github"
      ]
    },
    "buildDependenciesFromSource": true
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "eslint": "^7.23.0"
  },
  "scripts": {
    "start": "electron .",
    "pack-win": "electron-builder --win",
    "pack-win-test": "electron-builder --dir --win",
    "pack-linux-test": "electron-builder --dir --linux",
    "pack-linux": "electron-builder --linux",
    "pack-mac": "electron-builder --mac",
    "publish": "electron-builder --win -p always",
    "publish-linux": "electron-builder --linux -p always",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder"
  },
  "author": "",
  "license": "ISC"
}
