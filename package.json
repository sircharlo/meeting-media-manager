    "fs-extra": "^11.3.0",
    "heic-convert": "^2.1.0",
    "image-size": "^2.0.2",
    "is-online": "^11.0.0",
    "mime": "^4.0.7",
    "music-metadata": "^11.7.1",
    "obs-websocket-js": "^5.0.6",
