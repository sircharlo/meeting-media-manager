  },
  "resolutions": {
    "vite": "7.2.6"
  },
  "browserslist": [
    "Electron >= 37"
  ],
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{js,ts,cjs,mjs,mts,vue}": "eslint --fix"
  },
  "engines": {
    "node": "^22.19.0",
    "yarn": "^4.7.0"
