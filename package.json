    "@types/lodash-es": "^4.17.12",
    "@types/mousetrap": "^1.6.15",
    "@types/node": "^22.18.6",
    "@types/pretty-bytes": "^5.2.0",
    "@typescript-eslint/utils": "^8.44.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.12",
    "@vue/eslint-config-prettier": "^10.2.0",
