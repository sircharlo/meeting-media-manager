{
  "name": "jw-meeting-media-fetcher",
  "version": "0.9.0",
  "description": "JW Meeting Media Fetcher",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircharlo/jw-meeting-media-fetcher.git"
  },
  "dependencies": {
    "@electron/remote": "^1.2.1",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@popperjs/core": "^2.9.3",
    "adm-zip": "^0.5.5",
    "aspectratio": "^2.2.2",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "bootstrap-dark-5": "^1.1.0",
    "dayjs": "^1.10.6",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.3.10",
    "flatpickr": "^4.6.9",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.7",
    "graceful-fs": "^4.2.8",
    "h264-mp4-encoder": "^1.0.12",
    "i18n": "^0.13.3",
    "image-size": "^1.0.0",
    "is-port-reachable": "^3.0.0",
    "jquery": "^3.6.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.9.359",
    "select2": "^4.1.0-rc.0",
    "sql.js": "^1.6.1",
    "webdav": "^4.7.0"
  },
  "build": {
    "appId": "sircharlo.jw-meeting-media-fetcher",
    "productName": "JW Meeting Media Fetcher",
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "category": "Utilities",
      "target": "AppImage",
      "icon": "build/icon.png",
      "publish": [
        "github"
      ]
    },
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "universal"
        ]
      },
      "publish": [
        "github"
      ]
    },
    "buildDependenciesFromSource": true
  },
  "devDependencies": {
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0"
  },
  "scripts": {
    "start-debug": "electron .  --trace-warnings > ./app-run.log  2>&1",
    "start": "electron .",
    "pack-win": "electron-builder --win",
    "pack-win-test": "electron-builder --dir --win",
    "pack-linux-test": "electron-builder --dir --linux",
    "pack-linux": "electron-builder --linux",
    "pack-mac": "electron-builder --mac",
    "publish": "electron-builder --win -p always",
    "publish-linux": "electron-builder --linux -p always",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder"
  },
  "author": "",
  "license": "ISC"
}
