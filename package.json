    "msw": "^2.11.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.9.2",
    "vite": "^7.1.7",
    "vitepress": "^2.0.0-alpha.12",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.8",
    "webpack-iconfont-plugin-nodejs": "^1.0.36"
  },
  "resolutions": {
    "vite": "7.1.7"
  },
  "browserslist": [
    "Electron >= 37"
  ],
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{js,ts,cjs,mjs,mts,vue}": "eslint --fix"
  },
  "engines": {
    "node": "^22.17.1",
    "yarn": "^4.7.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.9.4",
  "productName": "Meeting Media Manager"
}
